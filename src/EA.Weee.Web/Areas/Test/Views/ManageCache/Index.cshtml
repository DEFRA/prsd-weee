@using EA.Weee.Web.RazorHelpers
@{
    ViewBag.Title = "Manage Cache";
}

@section main_content
{
    <header class="hgroup">
        <h1 class="heading-large">
            Manage Cache
        </h1>
    </header>

    <p>Use this page to clear the cache of producer search results.</p>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        @(this.WeeeGds().Submit("Continue", new { @class = "button" }))
    }

    <p>
        @Html.ActionLink("Back", "Index", "Home", null, null)
    </p>
}