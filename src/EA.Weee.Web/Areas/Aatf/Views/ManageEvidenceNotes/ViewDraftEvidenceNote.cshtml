
@using EA.Weee.Web.Areas.Aatf.Controllers
@using EA.Weee.Web.RazorHelpers
@using MvcNavigationHelpers
@model EA.Weee.Web.Areas.Aatf.ViewModels.EvidenceNoteViewModel
@{
    ViewBag.Title = "View Draft Evidence Note";
}

@{
    var text = string.Format("You have successfully saved the evidence note with reference ID E{0} as a draft", Model.Reference);
}
<section id="main-content">
    @(this.WeeeGds().BackLink(@Url.UrlFor<ManageEvidenceNotesController>(a => a.Index(Model.OrganisationId, Model.AatfId))))

    @(this.WeeeGds().Panel(text))

    @Html.Partial("_ViewEvidenceNotePartial", Model)
</section>
