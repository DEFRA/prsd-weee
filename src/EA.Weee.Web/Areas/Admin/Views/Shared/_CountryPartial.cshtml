@using EA.Prsd.Core.Web.Mvc.RazorHelpers
@model EA.Weee.Core.AatfReturn.AddressData

@Html.Gds().LabelFor(m => m.CountryId, new { id="country-dropdown-list" })
@Html.Gds().ValidationMessageFor(m => m.CountryId)
@Html.DropDownListFor(m => m.CountryId, new SelectList(Model.Countries, "Id", "Name"), string.Empty, new { @class = "govuk-!-width-one-third govuk-input", aria_labelledby = "country-dropdown-list" })