@model EA.Weee.Web.Areas.Admin.ViewModels.Scheme.Overview.OrganisationDetails.RegisteredCompanyDetailsOverviewViewModel

@Html.Partial("Overview/_OverviewHeaderAndTabs", Model)

<table id="pcs-details-table">
    <thead>
        <tr class="visuallyhidden">
            <th colspan="2">Organisation details</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th>Organisation Type</th>
            <td>Registered Company</td>
        </tr>
        <tr>
            <th>Company name</th>
            <td>@Model.CompanyName</td>
        </tr>
        <tr>
            <th>Company registration number (CRN)</th>
            <td>@Model.CompanyRegistrationNumber</td>
        </tr>
        <tr>
            <th>Business trading name</th>
            <td>@Model.BusinessTradingName</td>
        </tr>
        <tr>
            <th>Address</th>
            <td>
                @Model.Address.Address.Address1<br />
                @Model.Address.Address.Address2<br />
                @Model.Address.Address.TownOrCity<br />
                @Model.Address.Address.CountyOrRegion<br />
                @Model.Address.Address.Postcode<br />
                @Model.Address.Address.CountryName<br />
            </td>
        </tr>
        <tr>
            <th>Telephone</th>
            <td>@Model.Address.Address.Telephone</td>
        </tr>
        <tr>
            <th>Email address</th>
            <td>@Model.Address.Address.Email</td>
        </tr>
    </tbody>
</table>
