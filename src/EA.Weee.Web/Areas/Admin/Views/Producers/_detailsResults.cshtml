                        <td class="govuk-table__cell numeric">@scheme.ProducerEeeDetails.DisplayTonnage(QuarterSelection.Q4, WeeeCategory.PhotovoltaicPanels, ObligationType.B2C)</td>
                        <td class="govuk-table__cell numeric">@scheme.ProducerEeeDetails.DisplayTonnage(QuarterSelection.Q4, WeeeCategory.PhotovoltaicPanels, ObligationType.B2B)</td>
                    </tr>
                </tbody>
            </table>
        </section>
    </div>
}
<script type="text/javascript">
    $("a[id$='details-tab']").click(function (event) {
        event.preventDefault();

        var sectionId = $(this).attr('href').replace('#', '');

        $("section[id$='details']").each(function () {
            if ($(this).attr('id') !== sectionId) {
                $(this).addClass('govuk-tabs__panel--hidden');
            } else {
                $(this).removeClass('govuk-tabs__panel--hidden');
            }
        });

        var schemeIdentifier = $(this).attr('id')
            .replace('registration', '')
            .replace('contact', '')
            .replace('total-eee', '')
            .replace('q1', '')
            .replace('q2', '')
            .replace('q3', '')
            .replace('q4', '')
            .replace('-details-tab', '');

        // Inactivate scheme tags
        $("a[id$='details-tab']").each(function () {
            if ($(this).attr('id').indexOf(schemeIdentifier) !== -1) {
                $(this).removeClass('govuk-tabs__tab--selected');
            }
        });

        // Except this tab
        $(this).addClass('govuk-tabs__tab--selected');

        var tableIdentifier = $(this).attr('id').replace('details-tab', '');

        // Hide all data tables in scheme
        $("table[id$='details-table']").each(function () {
            if ($(this).attr('id').indexOf(schemeIdentifier) !== -1) {
                $(this).addClass('govuk-visually-hidden');
            }
        });

        // Except for the table corresponding to this tab
        $("table[id$='" + tableIdentifier + "details-table']").removeClass('govuk-visually-hidden');
    })
</script>
