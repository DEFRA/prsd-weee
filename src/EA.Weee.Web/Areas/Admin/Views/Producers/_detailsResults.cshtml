@using System.Linq
@using EA.Weee.Core.DataReturns
@using EA.Weee.Core.Shared
@using EA.Weee.Web.Extensions
@using EA.Weee.Core.Admin

@model ProducerDetails

@foreach (var scheme in Model.Schemes)
{
    <div class="govuk-tabs" data-module="govuk-tabs">
        <ul class="govuk-tabs__list">
            <li class="govuk-tabs__list-item">
                <a class="govuk-tabs__tab govuk-tabs__tab--selected" href="#registration-details" id="@(scheme.ObligationType)-registration-details-tab">Overview</a>
            </li>

            <li class="govuk-tabs__list-item">
                <a class="govuk-tabs__tab" href="#contact-details" id="@(scheme.ObligationType)-contact-details-tab">Contact details</a>
            </li>

            <li class="govuk-tabs__list-item">
                <a class="govuk-tabs__tab" href="#eee-details" id="@(scheme.ObligationType)-total-eee-details-tab">Total EEE</a>
            </li>

            @if (scheme.ProducerEeeDetails.Q1EEE.Any())
            {
                <li class="govuk-tabs__list-item">
                    <a class="govuk-tabs__tab" href="#q1-details" id="@(scheme.ObligationType)-q1-details-tab">Q1</a>
                </li>
            }

            @if (scheme.ProducerEeeDetails.Q2EEE.Any())
            {
                <li class="govuk-tabs__list-item">
                    <a class="govuk-tabs__tab" href="#q2-details" id="@(scheme.ObligationType)-q2-details-tab">Q2</a>
                </li>
            }

            @if (scheme.ProducerEeeDetails.Q3EEE.Any())
            {
                <li class="govuk-tabs__list-item">
                    <a class="govuk-tabs__tab" href="#q3-details" id="@(scheme.ObligationType)-q3-details-tab">Q3</a>
                </li>
            }

            @if (scheme.ProducerEeeDetails.Q4EEE.Any())
            {
                <li class="govuk-tabs__list-item">
                    <a class="govuk-tabs__tab" href="#q4-details" id="@(scheme.ObligationType)-q4-details-tab">Q4</a>
                </li>
            }
        </ul>

        <section class="govuk-tabs__panel" id="registration-details">
            <table id="@(scheme.ObligationType)-registration-details-table" class="govuk-table">
                <thead class="govuk-table__head">
                    <tr class="govuk-visually-hidden">
                        <th colspan="2">Overview</th>
                    </tr>
                </thead>
                <tbody class="govuk-table__body">
                    <tr class="govuk-table__row">
                        <th class="govuk-table__header" scope="col">Registered PCS</th>
                        <td class="govuk-table__cell">
                            @scheme.SchemeName
                            @if (Model.CanRemoveProducer)
                            {
                                <br />@Html.ActionLink("Remove producer", "ConfirmRemoval", "Producers", new { registeredProducerId = @scheme.RegisteredProducerId }, null)
                            }
                        </td>
                    </tr>
                    <tr class="govuk-table__row">
                        <th class="govuk-table__header" scope="col">Producer name</th>
                        <td class="govuk-table__cell">@scheme.ProducerName</td>
                    </tr>
                    <tr class="govuk-table__row">
                        <th class="govuk-table__header" scope="col">Trading name</th>
                        <td class="govuk-table__cell">@scheme.TradingName</td>
                    </tr>
                    <tr class="govuk-table__row">
                        <th class="govuk-table__header" scope="col">Registration date</th>
                        <td class="govuk-table__cell">@scheme.RegistrationDate.ToString("dd MMMM yyyy")</td>
                    </tr>
                    @if (@scheme.CompanyNumber != null)
                    {
                        <tr class="govuk-table__row">
                            <th class="govuk-table__header" scope="col">Company registration number</th>
                            <td class="govuk-table__cell">@scheme.CompanyNumber</td>
                        </tr>
                    }
                    <tr class="govuk-table__row">
                        <th class="govuk-table__header" scope="col">@(scheme.IsCompany ? "Registered office" : "Principal place of business")</th>
                        <td class="govuk-table__cell">@scheme.ProducerBusinessContact.Address</td>
                    </tr>
                    <tr class="govuk-table__row">
                        <th class="govuk-table__header" scope="col">Obligation type</th>
                        <td class="govuk-table__cell">@scheme.ObligationType</td>
                    </tr>
                    <tr class="govuk-table__row">
                        <th class="govuk-table__header" scope="col">Charge band type</th>
                        <td class="govuk-table__cell">@scheme.ChargeBandType</td>
                    </tr>
                    <tr class="govuk-table__row">
                        <th class="govuk-table__header" scope="col">Authorised representative</th>
                        <td class="govuk-table__cell">@scheme.IsAuthorisedRepresentative</td>
                    </tr>
                    @if (@scheme.CeasedToExist != null)
                    {
                        <tr class="govuk-table__row">
                            <th class="govuk-table__header" scope="col">Ceased to exist</th>
                            <td class="govuk-table__cell">@scheme.CeasedToExist.Value.ToString("dd MMMM yyyy")</td>
                        </tr>
                    }
                </tbody>
            </table>
