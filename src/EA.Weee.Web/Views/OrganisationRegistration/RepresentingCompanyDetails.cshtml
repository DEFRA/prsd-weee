@using EA.Weee.Core.DataStandards
@using EA.Weee.Core.Organisations
@using MvcNavigationHelpers
@using EA.Weee.Web.Controllers
@using EA.Weee.Web.RazorHelpers
@model EA.Weee.Core.Organisations.RepresentingCompanyDetailsViewModel
@{
    const string title = "Producer details";
    ViewBag.Title = title + " - GOV.UK";
}
@section full_main_content
{

    @this.WeeeGds().BackLink(@Url.UrlFor<OrganisationRegistrationController>(a => a.RepresentingCompanyRedirect(@ViewBag.ReturnUrl)))
    
    <header class="hgroup">
        <h1 class="govuk-heading-l">@title</h1>
    </header>

    @Html.Partial("_RepresentingCompanyPartial", Model)
}
