<!DOCTYPE html>
<html lang="@Html.Raw(ViewBag.HtmlLang ?? "en")" class="govuk-template gt-ie8">

<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="format-detection" content="telephone=no" />
    <meta name="theme-color" content="#0b0c0c" />
    <title>@Html.Raw(ViewBag.Title ?? "GOV.UK - The best place to find government services and information")</title>

    <link rel="shortcut icon" href="@Url.Content("~/Content/govuk_frontend/images/favicon.ico")" type="image/x-icon" />
    <link rel="shortcut icon" sizes="16x16 32x32 48x48" href="@Url.Content("~/Content/govuk_frontend/images/favicon.ico")" type="image/x-icon" />
    <link rel="mask-icon" href="@Url.Content("~/Content/govuk_frontend/images/govuk-mask-icon.svg")" color="#0b0c0c" />
    <link rel="apple-touch-icon" sizes="180x180" href="@Url.Content("~/Content/govuk_frontend/images/govuk-apple-touch-icon-180x180.png")" />
    <link rel="apple-touch-icon" sizes="167x167" href="@Url.Content("~/Content/govuk_frontend/images/govuk-apple-touch-icon-167x167.png")" />
    <link rel="apple-touch-icon" sizes="152x152" href="@Url.Content("~/Content/govuk_frontend/images/govuk-apple-touch-icon-152x152.png")" />
    <link rel="apple-touch-icon" href="@Url.Content("~/Content/govuk_frontend/images/govuk-apple-touch-icon.png")" />

    <meta property="og:image" content="@Url.Content("~/Content/govuk_frontend/images/opengraph-image.png")" />
    @RenderSection("head", required: false)
</head>

<body class="govuk-template__body @Html.Raw(ViewBag.BodyClasses ?? string.Empty)">
    <script>
        document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');
    </script>

    @RenderSection("body_start", required: false)

    <div id="skiplink-container">
        <div>
            <a href="#main-content" class="govuk-skip-link skiplink">@Html.Raw(ViewBag.SkipLinkMessage ?? "Skip to main content")</a>
        </div>
    </div>

    <!--start global-cookie-message-->
    <div id="global-cookie-message" style="display:none;">
        <p id="global-cookie-message-p1">Cookies on Waste Electrical and Electronic Equipment</p>
        <p id="global-cookie-message-p2">We also use essential cookies to remember if you've accepted analytics cookies.</p>
        <p id="global-cookie-message-p3">We'd like to use analystics cookies so we can understand how you use Waste Electrical and Electronic Equipment and make improvements.</p>
        <p id="#global-cookie-message-p4">
            <input type="submit" id="acceptCookies" value="Accept additional cookies" class="govuk-cookie-button">
            <input type="submit" id="rejectCookies" value="Reject additional cookies" class="govuk-cookie-button">
            @Html.ActionLink("View cookies", "Cookies", "NewUser", new { area = string.Empty }, null)
        </p>
    </div>

    <div id="global-cookie-accept-message" style="display:none;">
        <p id="global-cookie-message-p2">
            You have accepted additional cookies. You can @Html.ActionLink("change your cookie settings", "Cookies", "NewUser", new { area = string.Empty }, null) at any time.
        </p>
        <p>
            <input type="submit" id="hideAcceptCookies" value="Hide this message" class="govuk-cookie-button">
        </p>
    </div>

    <div id="global-cookie-reject-message" style="display:none;">
        <p id="global-cookie-message-p2">
            You have rejected additional cookies. You can @Html.ActionLink("change your cookie settings", "Cookies", "NewUser", new { area = string.Empty }, null) at any time.
        </p>
        <p>
            <input type="submit" id="hideRejectCookies" value="Hide this message" class="govuk-cookie-button">
        </p>
    </div>
    <!--end global-cookie-message-->


    <header class="govuk-header @Html.Raw(ViewBag.HeaderClass ?? string.Empty)" role="banner" data-module="header">
        <div class="govuk-header__container govuk-width-container">
            <div class="header-global">
                <div class="govuk-header__logo">
                    <a href="@Html.Raw(ViewBag.HomepageUrl ?? "https://www.gov.uk/")" title="@Html.Raw(ViewBag.LogoLinkTitle ?? "Go to the GOV.UK homepage")" id="logo" class="govuk-header__link govuk-header__link--homepage">
                        <span class="govuk-header__logotype">

                            <svg role="presentation" focusable="false" class="govuk-header__logotype-crown" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 132 97" height="32" width="36">
                                <path fill="currentColor" fill-rule="evenodd" d="M25 30.2c3.5 1.5 7.7-.2 9.1-3.7 1.5-3.6-.2-7.8-3.9-9.2-3.6-1.4-7.6.3-9.1 3.9-1.4 3.5.3 7.5 3.9 9zM9 39.5c3.6 1.5 7.8-.2 9.2-3.7 1.5-3.6-.2-7.8-3.9-9.1-3.6-1.5-7.6.2-9.1 3.8-1.4 3.5.3 7.5 3.8 9zM4.4 57.2c3.5 1.5 7.7-.2 9.1-3.8 1.5-3.6-.2-7.7-3.9-9.1-3.5-1.5-7.6.3-9.1 3.8-1.4 3.5.3 7.6 3.9 9.1zm38.3-21.4c3.5 1.5 7.7-.2 9.1-3.8 1.5-3.6-.2-7.7-3.9-9.1-3.6-1.5-7.6.3-9.1 3.8-1.3 3.6.4 7.7 3.9 9.1zm64.4-5.6c-3.6 1.5-7.8-.2-9.1-3.7-1.5-3.6.2-7.8 3.8-9.2 3.6-1.4 7.7.3 9.2 3.9 1.3 3.5-.4 7.5-3.9 9zm15.9 9.3c-3.6 1.5-7.7-.2-9.1-3.7-1.5-3.6.2-7.8 3.7-9.1 3.6-1.5 7.7.2 9.2 3.8 1.5 3.5-.3 7.5-3.8 9zm4.7 17.7c-3.6 1.5-7.8-.2-9.2-3.8-1.5-3.6.2-7.7 3.9-9.1 3.6-1.5 7.7.3 9.2 3.8 1.3 3.5-.4 7.6-3.9 9.1zM89.3 35.8c-3.6 1.5-7.8-.2-9.2-3.8-1.4-3.6.2-7.7 3.9-9.1 3.6-1.5 7.7.3 9.2 3.8 1.4 3.6-.3 7.7-3.9 9.1zM69.7 17.7l8.9 4.7V9.3l-8.9 2.8c-.2-.3-.5-.6-.9-.9L72.4 0H59.6l3.5 11.2c-.3.3-.6.5-.9.9l-8.8-2.8v13.1l8.8-4.7c.3.3.6.7.9.9l-5 15.4v.1c-.2.8-.4 1.6-.4 2.4 0 4.1 3.1 7.5 7 8.1h.2c.3 0 .7.1 1 .1.4 0 .7 0 1-.1h.2c4-.6 7.1-4.1 7.1-8.1 0-.8-.1-1.7-.4-2.4V34l-5.1-15.4c.4-.2.7-.6 1-.9zM66 92.8c16.9 0 32.8 1.1 47.1 3.2 4-16.9 8.9-26.7 14-33.5l-9.6-3.4c1 4.9 1.1 7.2 0 10.2-1.5-1.4-3-4.3-4.2-8.7L108.6 76c2.8-2 5-3.2 7.5-3.3-4.4 9.4-10 11.9-13.6 11.2-4.3-.8-6.3-4.6-5.6-7.9 1-4.7 5.7-5.9 8-.5 4.3-8.7-3-11.4-7.6-8.8 7.1-7.2 7.9-13.5 2.1-21.1-8 6.1-8.1 12.3-4.5 20.8-4.7-5.4-12.1-2.5-9.5 6.2 3.4-5.2 7.9-2 7.2 3.1-.6 4.3-6.4 7.8-13.5 7.2-10.3-.9-10.9-8-11.2-13.8 2.5-.5 7.1 1.8 11 7.3L80.2 60c-4.1 4.4-8 5.3-12.3 5.4 1.4-4.4 8-11.6 8-11.6H55.5s6.4 7.2 7.9 11.6c-4.2-.1-8-1-12.3-5.4l1.4 16.4c3.9-5.5 8.5-7.7 10.9-7.3-.3 5.8-.9 12.8-11.1 13.8-7.2.6-12.9-2.9-13.5-7.2-.7-5 3.8-8.3 7.1-3.1 2.7-8.7-4.6-11.6-9.4-6.2 3.7-8.5 3.6-14.7-4.6-20.8-5.8 7.6-5 13.9 2.2 21.1-4.7-2.6-11.9.1-7.7 8.8 2.3-5.5 7.1-4.2 8.1.5.7 3.3-1.3 7.1-5.7 7.9-3.5.7-9-1.8-13.5-11.2 2.5.1 4.7 1.3 7.5 3.3l-4.7-15.4c-1.2 4.4-2.7 7.2-4.3 8.7-1.1-3-.9-5.3 0-10.2l-9.5 3.4c5 6.9 9.9 16.7 14 33.5 14.8-2.1 30.8-3.2 47.7-3.2z"></path>
                            </svg>
                            <span class="govuk-header__logotype-text">
                                @Html.Raw(ViewBag.GlobalHeaderText ?? "GOV.UK")
                            </span>
                        </span>
                    </a>
                </div>
                @RenderSection("inside_header", required: false)

            </div>
            @RenderSection("proposition_header", required: false)
        </div>
    </header>

    @RenderSection("after_header", required: false)

    @RenderBody()

    <footer class="govuk-footer " role="contentinfo" id="footer">
        <div class="govuk-width-container ">
            @RenderSection("footer_top", required: false)
            <hr class="govuk-footer__section-break">
            <div class="govuk-footer__meta">
                <div class="govuk-footer__meta-item govuk-footer__meta-item--grow govuk-!-margin-left-6">
                    @RenderSection("footer_support_links", required: false)
                    <svg role="presentation" focusable="false" class="govuk-footer__licence-logo" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 483.2 195.7" height="17" width="41">
                        <path fill="currentColor" d="M421.5 142.8V.1l-50.7 32.3v161.1h112.4v-50.7zm-122.3-9.6A47.12 47.12 0 0 1 221 97.8c0-26 21.1-47.1 47.1-47.1 16.7 0 31.4 8.7 39.7 21.8l42.7-27.2A97.63 97.63 0 0 0 268.1 0c-36.5 0-68.3 20.1-85.1 49.7A98 98 0 0 0 97.8 0C43.9 0 0 43.9 0 97.8s43.9 97.8 97.8 97.8c36.5 0 68.3-20.1 85.1-49.7a97.76 97.76 0 0 0 149.6 25.4l19.4 22.2h3v-87.8h-80l24.3 27.5zM97.8 145c-26 0-47.1-21.1-47.1-47.1s21.1-47.1 47.1-47.1 47.2 21 47.2 47S123.8 145 97.8 145" />
                    </svg>
                    <span class="govuk-footer__licence-description">
                        @Html.Raw(ViewBag.LicenceMessage ?? "All content is available under the <a class=\"govuk-footer__link\" href=\"https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/\" rel=\"license\">Open Government Licence v3.0</a>, except where otherwise stated")
                    </span>
                </div>
                <div class="govuk-footer__meta-item">
                    <a class="footer-logo govuk-footer__link " href="https://www.nationalarchives.gov.uk/information-management/re-using-public-sector-information/uk-government-licensing-framework/crown-copyright/">
                        <img src="@Url.Content("~/Content/govuk_frontend/images/govuk-crest.png")" alt="govuk crest" /><br /> @Html.Raw(ViewBag.CrownCopyrightMessage ?? "&copy; Crown copyright")
                    </a>
                </div>
            </div>
        </div>
    </footer>


    <div hidden="hidden" class="hidden-for-screen-reader">
        Version: @Startup.ApplicationVersion
    </div>

    <div id="global-app-error" class="app-error hidden"></div>

    <script src="@Url.Content("~/Scripts/govuk/govuk-template.js")"></script>

    @RenderSection("body_end", required: false)

</body>

</html>

<script>
    var self = this;
    var cookieConsent = GOVUK.getCookie("weee-cookie-consent");

    if (cookieConsent === null || cookieConsent === "null") {
        document.getElementById("global-cookie-message").style.display = "block";
    } else {
        document.getElementById("global-cookie-message").style.display = "none";
    }

    $("#acceptCookies").click(function () {
        GOVUK.cookie('weee-cookie-consent', 'yes', { days: 28 });
        self.startGoogleTracking();
        document.getElementById("global-cookie-message").style.display = "none";
        document.getElementById("global-cookie-accept-message").style.display = "block";
    });

    $("#rejectCookies").click(function () {
        GOVUK.cookie('weee-cookie-consent', 'no', { days: 28 });
        document.getElementById("global-cookie-message").style.display = "none";
        GOVUK.cookie('_ga', null);
        GOVUK.cookie('_gid', null);
        GOVUK.cookie('_gat', null);
        GOVUK.cookie('_gat_govuk_shared', null);
        document.getElementById("global-cookie-reject-message").style.display = "block";
    });

    $("#hideAcceptCookies").click(function () {
        document.getElementById("global-cookie-accept-message").style.display = "none";
    });

    $("#hideRejectCookies").click(function () {
        document.getElementById("global-cookie-reject-message").style.display = "none";
    });

    self.startGoogleTracking = function () {
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
                m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
        ga('create', 'UA-145652997-1', 'auto', 'govuk_shared', { 'allowLinker': true });
        ga('govuk_shared.require', 'linker');
        ga('govuk_shared.linker.set', 'anonymizeIp', true);
        ga('govuk_shared.linker.set', 'allowAdFeatures', false);
        ga('govuk_shared.linker:autoLink', ['www.gov.uk', 'www.nationalarchives.gov.uk', 'www.daera-ni.gov.uk', 'www.data.gov.uk']);
        ga('send', 'pageview');
        ga('govuk_shared.send', 'pageview');
    }

    var element = document.getElementById('saveCookieChoice');
    if (typeof (element) != 'undefined' && element != null) {
        if (cookieConsent && cookieConsent === "yes") {
            document.getElementById("cookieChoiceYes").checked = true;
        } else {
            document.getElementById("cookieChoiceNo").checked = true;
        }
    }

    function acceptCookies() {
        GOVUK.cookie('weee-cookie-consent', 'yes', { days: 28 });
        self.startGoogleTracking();
        document.getElementById("global-cookie-message").style.display = "none";
    }

    function rejectCookies() {
        deleteCookies();

        GOVUK.cookie('weee-cookie-consent', 'no', { days: 28 });
        document.getElementById("global-cookie-message").style.display = "none";
        GOVUK.cookie('_ga', null);
        GOVUK.cookie('_gid', null);
        GOVUK.cookie('_gat', null);
        GOVUK.cookie('_gat_govuk_shared', null);
    }

    function setCookieChoice() {
        if (document.getElementById("cookieChoiceYes").checked === false && document.getElementById("cookieChoiceNo").checked === false) {
            return;
        }        

        if (document.getElementById("cookieChoiceYes").checked) {
            acceptCookies();
        } else {
            rejectCookies();
        }

        window.scrollTo({ top: 0 });
        document.getElementById("dvSuccessMessage").style.display = "block";
        document.getElementById("global-cookie-accept-message").style.display = "none";
        document.getElementById("global-cookie-reject-message").style.display = "none";
    }

    function deleteCookies() {
        var allCookies = document.cookie.split(';');

        // The "expire" attribute of every cookie is 
        // Set to "Thu, 01 Jan 1970 00:00:00 GMT"
        for (var i = 0; i < allCookies.length; i++) {
            document.cookie = allCookies[i] + "=;expires=" + new Date(0).toUTCString();
        }
    }
</script>