
@model EA.Weee.Web.ViewModels.Shared.YesNoChoiceViewModel
@{
    ViewBag.Title = "Do you need a user account?";
}
@section main_content
{
    <header class="hgroup">
        <h1 class="heading-large">
            Do you want to perform activities for the 2016 compliance year?
        </h1>
    </header>
    @using (Html.BeginForm(actionName: "CheckComplianceYear", controllerName: "NewUser", method: FormMethod.Post))
    {
        @Html.AntiForgeryToken()

        
        <div class="form-group @Html.Gds().FormGroupClass(m => m.Choices.SelectedValue)">
            @Html.EditorFor(m => m.Choices)
        </div>
        <div class="form-group">
            <button class="button" type="submit">Continue</button>
        </div>
    }
}